
programa
{
	funcao inicio()
	{
		inteiro n, c
		
		c = 1
		
		escreva("Digite um número: ")
		leia(n)
		
		enquanto ( n >= c)
		{
			escreva(c + " ")
			c = c + 2
		}
		escreva("\n Fim! ")
	}
}
